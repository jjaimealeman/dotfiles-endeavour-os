source ~/.config/zsh/.zshrc
c 'what is the meaning of life'
c '\
the following script works like this. the user types the letter C followed by a pair of single quotes. within the single quotes, the user enters their prompt.\
this is the format of the prompt: c 'what is the meaning of life?'\
please rewrite the following script/function and take the users input and assign it to a variable called "QWERTY" that can be used elsewhere in the function.\
\
alias c='f() {\
curl --silent https://api.openai.com/v1/chat/completions \\
 -H "Authorization: Bearer $CHAT_GPT_API_KEY" \\
 -H "Content-Type: application/json" \\
 -d "{ \"model\": \"gpt-3.5-turbo\", \"messages\": [{\"role\": \"user\", \"content\": \"$@\"}]}" \\
 | jq -r ".choices[0].message.content" \\
 | tee ~/.config/zsh/chat/latest-prompt.md \\
 | glow \\
}; \\
printf "\n"; \\
printf "  +++++++++++++++++++++++++++\n"; \\
printf "  + ChatGPT is thinking ... +\n"; \\
printf "  +++++++++++++++++++++++++++\n"; \\
;f''
z zsh
